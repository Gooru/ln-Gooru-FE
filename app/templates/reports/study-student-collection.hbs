{{player.gru-study-header
  classId=classId
  collection=collection
  courseId=course.id
  courseTitle=course.title
  showConfirmation=false
  fromReport=true
  class=class
  sourceType=source
}}

<div class="report-container {{if isShowSuggestion 'show-suggestion'}}">
  <div class="summary-reset">
    {{#if enableConfetti }}
      {{gru-confetti}}
    {{/if}}
    {{reports.qz-student-report attemptData=attemptData profile=profile}}
  </div>
  {{player.gru-player-summary-footer isDone=isDone classId=classId parents="next" courseId=courseId collection=collection isFullScreen=isFullScreen onToggleScreen="onToggleScreen"}}
</div>

{{#if isShowSuggestion}}
  {{content.modals.gru-content-suggestion mapLocation=mapLocation onAcceptSuggestion="onAcceptSuggestion" onIgnoreSuggestion="onIgnoreSuggestion"}}
{{/if}}
