{{#each questionItems as |item|}}
  <div class="item" {{action 'openFRQuestionGrade' item}}>
    <div class="question ellipsis">
      <span class="rubric-icon"><i class="icon"></i></span>
      <span class="question-name">{{item.question.title}}</span>
    </div>
  </div>
{{/each}}
{{#if showFRQuestionGrade}}
  {{grade.pull-up.free-response-question-grade isDCAContext=isDCAContext showPullUp=showFRQuestionGrade context=itemToGradeContextData itemsToGrade=questionItems}}
{{/if}}
