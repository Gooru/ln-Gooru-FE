<div class="modal-header delete">
  <h4 class="modal-title">Archive classroom</h4>
</div>
<div class="modal-body">
  <form>
    <p class="legend">{{t "content.modals.archive-class.legend"}}
        <b>
          {{model.content.title}}
        </b>
    </p>
    <div class="delete-info">
      <ul>
        <li>
          <label>
            {{input type="checkbox" checked=validator.check1}}
            <span>{{t "content.modals.archive-class.links-not-accessible"}}</span>
          </label>
        </li>
        <li>
          <label>
            {{input type="checkbox" checked=validator.check2}}
            <span>{{t "content.modals.archive-class.students-no-access"}}</span>
          </label>
        </li>
       {{#unless hasNoWarning}}
        <li>
          <label>
            {{input type="checkbox" checked=validator.check3}}
            <span>{{t "content.modals.archive-class.not-add-students"}}</span>
          </label>
        </li>
       {{/unless}}
      </ul>
    </div>
    <p class="confirmation">{{t "content.modals.archive-class.confirmation"}}</p>

    <div class="actions delete">
      <button type="button" class="btn btn-default cancel" data-dismiss="modal" aria-label="{{t 'common.close'}}">
        <span aria-hidden="true">{{t 'common.cancel'}}</span>
      </button>
      {{#gru-spinner-button classes='btn btn-warning delete' text=(t 'common.loading') isLoading=isLoading}}
          <button class="btn btn-warning archive" disabled={{isDisabled}} {{action "archiveClass" model}}>{{t 'common.archive'}}</button>
      {{/gru-spinner-button}}
    </div>
  </form>
</div>