<div class="header-tab-panel">
  <div class="nav-tabs">
    <div class="tab {{if (eq selectedItem 'metadata') 'active'}}" {{action 'selectItem' 'metadata'}}>METADATA</div>
    <div class="tab {{if (eq selectedItem 'journey') 'active'}}" {{action 'selectItem' 'journey'}}>JOURNEY</div>
    <div class="tab {{if (eq selectedItem 'learning') 'active'}}" {{action 'selectItem' 'learning'}}>LEARNING MAP</div>
  </div>
</div>
<div class="panel-content">
  {{#if (eq selectedItem 'journey')}}
    {{reports.pull-up.student-journey  collectionPerformances=collectionPerformances userId=userId}}
  {{else if (eq selectedItem 'metadata')}}
    {{reports.pull-up.competency-meta-data  microCompetencies=microCompetencies prequisiteCompetencies=prequisiteCompetencies }}
  {{else}}
    {{reports.pull-up.learning-map-content learningMapData=learningMapData onSelectActivityContent="onSelectActivityContent" isStudent=isStudent learningMapData=learningMapData}}
  {{/if}}
</div>
