<div class="container">
  <div class="card-container">
    {{#if (and (not session.isAnonymous)  (eq session.role 'teacher'))}}
      {{#link-to "library-search" (query-params type="my-content" profileId=session.userId)}}
        <div class="card-panel">
          <div class="title">{{t 'my-content'}}</div>
          <i class="image my-content"></i>
        </div>
      {{/link-to}}
    {{/if}}
    {{#link-to "library-search" (query-params type="gooru-catalog")}}
      <div class="card-panel">
        <div class="title">{{t 'common.gooru-catalog'}}</div>
        <i class="image catalog"></i>
      </div>
    {{/link-to}}
  </div>
  <div class="partner-libraries">
    <div class="heading"><span>{{t 'featured-libraries'}}</span></div>
    <div class="libraries-container">
      {{#each libraries as |library|}}
        {{library.gru-partner-library-card content=library}}
      {{/each}}
    </div>
  </div>
</div>
