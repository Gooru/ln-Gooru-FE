                </div>
              {{/each}}
              <div class='sub-sec-row' style='display:none;'>
                <div class='add-co-teacher-icon' {{action 'addCoteacher' }}> {{gru-icon name='add'}} </div>
                <div class='add-co-teacher-label'> {{t
                  'teacher-landing.class.class-settings.class-settings-sec.add-coteacher'}}</div>
              </div>
              <div class='row-offline-class sub-sec-row'>
                <div class='offline-icon'></div>
                <div class='offline-label'> {{t
                  'teacher-landing.class.class-settings.class-settings-sec.offline-class-label'}}</div>
                <div class='action-button-slide-yes'>
                  {{gru-switch switchOptions=switchOptions onOptionSwitch='saveOffline'
                  isChecked=class.isOffline}}
                </div>
              </div>
            </div>
          </div>
        </div>
        {{#if (and course subject) }}
