                        {{/if}}
                      </ul>
                    </div>
                  </div>
                </div>
                <div class='destination' class="{{if student.isActive "" "inActiveRow"}}  ">
                  <div class='grade-info'>
                    <div class="dropdown">
                      <button class="btn btn-default dropdown-toggle" type="button" data-toggle="dropdown"
                        aria-haspopup="true" aria-expanded="true"
                        {{action 'prepareListData' student 'student-destination'}}>
                        {{#if (and student.gradeUpperBound ( not (eq student.gradeUpperBound 'undefined' ) ))}}
                          {{get-value 'source' source=subjectTaxonomyGrades searchProp='id'
                        searchVal=student.gradeUpperBound
                        destProp='grade' }}
                        {{else}}
                          {{t 'teacher-landing.class.class-settings.class-settings-sec.option-choose-one'}}
                        {{/if}}
                        <span class="caret"></span>
                      </button>
                      <ul class="dropdown-menu">
                        {{#if getCurrentGradeDDContent}}
                          {{#each currentFilterList as |grade|}}
                            {{#if grade}}
                              <li> <a data-item={{grade}}
                                  {{ action 'updateClassMembersSettings'  student grade.id 'grade_upper_bound'}}>{{grade.grade}}
                                </a> </li>
                            {{/if}}
                          {{/each}}
                        {{/if}}
                      </ul>
                    </div>
                  </div>
                </div>
              {{else }}
                <div class="student-name {{if student.isActive "" "inActiveRow"}}  " style="grid-column-start:2; grid-column-end:6;">
                  <span class="ellipsis">{{student.lastName}} {{student.firstName}}</span>
                </div>
              {{/if}}
              <div class="student-id {{if student.isActive "" "inActiveRow"}}  "> {{student.studentId}} </div>
              {{!-- <div class='joined'> </div> --}}
              {{#if (and (and course subject) isPremiumClass) }}
                <div class=" student-action-col {{if student.isActive "" "inActiveRow"}}  ">
                  <div class='student-adj-lp' {{action 'applyClassMembersSettings' student}} title="{{t 'teacher-landing.class.class-settings.class-settings-sec.generate-pathway'}}"
                    data-toggle="tooltip"> {{gru-icon name='trending_up'}}</div>
                </div>
              {{/if}}
              <div class='student-action-col'>
                <div class='student-act-de' {{action "classMembersToggle" student}} title="Activate/Deactivate"
                  data-toggle="tooltip">
                  {{gru-icon name='block'}}</div>
              </div>
              <div class='student-action-col'>
                <div class='student-delete' {{action "onRemoveStudent" student}} title="{{t 'common.remove'}}"
                  data-toggle="tooltip">
                  {{gru-icon name='delete'}}</div>
              </div>
            {{/each}}
          </div>
          <div class='sub-sec-row'>
            <div class='add-student-icon'> {{gru-icon name='add'}}</div>
            <div class='add-student-label'> {{t 'teacher-landing.class.class-settings.action-lable-add-student'}}</div>
          </div>
        </div>
      </div>
    </div> {{!-- two col layout ends --}}
  </div>
</div>