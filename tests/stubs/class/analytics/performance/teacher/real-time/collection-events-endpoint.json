[
  {
    "description": "RealTime API: This endpoint returns all the events registered in the RealTime server for a specific collection in a specific class",
    "request": {
      "url": "^/nucleus/realtime/class/(.+)/collection/(.+)/events",
      "method": "GET"
    },
    "response": {
      "status": 200,
      "headers": {
        "Content-Type": "application/json"
      },
      "body": "File not found!",
      "file": "collection-events-200-response.json"
    }
  },
  {
    "description": "RealTime API: This endpoint emulates a RealTime POST event.",
    "request": {
      "url": "^/nucleus/realtime/class/(.+)/collection/(.+)/user/(.+)/event",
      "method": "POST"
    },
    "response": {
      "status": 200,
      "headers": {
        "Content-Type": "application/json"
      },
      "body": ""
    }
  },
  {
    "description": "RealTime API: This endpoint emulates a RealTime POST for collection event.",
    "request": {
      "url": "^/nucleus/realtime/class/(.+)/collection/(.+)/user/(.+)/complete",
      "method": "POST"
    },
    "response": {
      "status": 200,
      "headers": {
        "Content-Type": "application/json"
      },
      "body": ""
    }
  }
]
